    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "^0.378.0",
    "multer": "^1.4.5-lts.1",
    "next": "^15.1.6",
    "next-sitemap": "^4.0.6",
    "nodemailer": "^6.10.0",
    "open-graph-scraper": "^6.9.0",
    "payload": "^3.28.1",
    "payload-admin-bar": "^1.0.6",
