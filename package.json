    "prettier": "3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "@swc/core",
      "es5-ext",
      "esbuild"
    ]
  }
}
