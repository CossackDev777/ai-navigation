    "@payloadcms/eslint-config": "3.28.0",
    "@payloadcms/eslint-plugin": "3.28.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.13",
    "@types/escape-html": "^1.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "22.10.2",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
